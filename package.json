{
  "name": "react-confetti-shooter",
  "version": "1.1.6",
  "type": "module",
  "description": "A simple, one-line React confetti library with directional shooting",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "bun run build",
    "release": "./scripts/release.sh",
    "release:patch": "./scripts/release.sh patch",
    "release:minor": "./scripts/release.sh minor",
    "release:major": "./scripts/release.sh major"
  },
  "keywords": [
    "react",
    "confetti",
    "animation",
    "particles",
    "celebration",
    "typescript",
